
      <div class="row"><h1>Latest <%= @category %> News</h1> </div>
      <div class="row">
        <article class="span12">
          <div class="inner-1">
            <ul class="list-blog">
                <% @articles.each do |article| %>
                <li>
                  <%= render partial: 'articles/individual_article', locals: {:article => article} %>
                </li>
                <% end %>
            </ul>
          </div>
          <%= will_paginate @articles %>
      </article>
    </div>
