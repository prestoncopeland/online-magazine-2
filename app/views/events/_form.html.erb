<div class="spinner"></div>
<div class="devise-form">

<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :description, input_html: {:size => "30x10"} %>
    <%= f.input :category %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :telephone %>
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => "btn btn-1" %>
  </div>
<% end %>
<%= link_to 'Go back to all events', events_path %>

</div>



